<launch>
	<arg name="veh" default="$(env VEHICLE_NAME)"/>
	<arg name="verbose" default="false"/>

	<!-- start basic args -->
	<include file="$(find duckietown_demos)/launch/master.launch">
		<!-- Basic parameters -->
		<arg name="veh" value="$(arg veh)"/>
		<arg name="visualization" value="true" />
        <arg name="verbose" value="$(arg verbose)" />

		<arg name="demo_name" default="apriltag_demo"/>

		<!-- Basic functionalities -->
		<arg name="camera" value="true"/>
		<arg name="/camera/raw" value="true"/>
		<arg name="/camera/raw/rect-nodelet" value="true"/>
		<arg name="apriltags" value="true" />
	</include>
</launch>
